algoritmo "para"

var
cont, num, soma : inteiro

inicio
soma <- 0

      para cont <- 1 ate 5 faca
      
           escreva ("digite um numnero: ")
           leia(num)
           
           soma <- soma + num
           cont <- cont +1
      fimpara

      escreva ("a soma dos numeros e " , soma )

fimalgoritmo