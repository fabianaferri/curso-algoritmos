algoritmo "ConteAte10"

var

valor : inteiro

cont : inteiro




inicio
cont <- 0

         Escreva("quer contar ate quanto? ")
         leia (valor)

Enquanto (cont <= valor) faca

         escreval(cont)
         cont <- cont + 1

fimenquanto

           escreval ("terminei de contar")


fimalgoritmo