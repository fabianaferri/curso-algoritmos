algoritmo "CalculaIdade"
var

       ano, nasc, idade :inteiro

inicio

Escreva("em que ano espamos?")
Leia (ano)
Escreva("em que ano vc nasceu?")
Leia (nasc)
     idade <- ano - nasc

Escreval ("em " , ano , " vc tera ", idade, " anos" )

Se (idade >= 21) entao
   Escreval ("parabens vc é maior de idade")
fimse



fimalgoritmo