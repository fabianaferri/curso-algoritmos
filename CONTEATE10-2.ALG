algoritmo "ConteAte10"

var


salto, valor , cont : inteiro


inicio
cont <- 0

         Escreva("quer contar ate quanto? ")
         leia (valor)
         escreva ("qual é o valor do salto? ")
         leia (salto)

Enquanto (cont <= valor) faca
         escreval(cont)
         cont <- cont + salto

fimenquanto

           escreval ("terminei de contar")


fimalgoritmo