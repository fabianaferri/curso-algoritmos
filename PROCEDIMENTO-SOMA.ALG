algoritmo "procedimento-soma"

var
v1, v2, soma : inteiro

   procedimento soma(a, b: inteiro)
   inicio
         escreval ("recebi o valor " , a)
         escreval ("recebi o valor " , b)
         escreval ("a soma dos valore e " , a+b)

   fimprocedimento



inicio
v1 <- 5
v2 <- 3
soma (v1, v2)

fimalgoritmo